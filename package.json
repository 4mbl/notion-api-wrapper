{
  "name": "notion-api-wrapper",
  "version": "5.0.3",
  "description": "Zero dependency Notion API client.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "4mbl",
  "license": "MIT",
  "keywords": [
    "notion",
    "api"
  ],
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint",
    "format:write": "prettier --write ./src/**/*.ts",
    "format:check": "prettier --check ./src/**/*.ts",
    "bump:patch": "npm version patch --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:major": "npm version major --no-git-tag-version"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@notionhq/client": "^3.1.1",
    "@types/node": "^22.15.21",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.1.4"
  },
  "packageManager": "pnpm@10.11.0"
}
