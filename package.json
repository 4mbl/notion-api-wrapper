{
  "name": "notion-api-wrapper",
  "version": "5.0.3",
  "description": "Zero dependency Notion API client.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "4mbl",
  "license": "MIT",
  "homepage": "https://github.com/4mbl/notion-api-wrapper#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/4mbl/notion-api-wrapper"
  },
  "keywords": [
    "notion",
    "notion api",
    "notion-api",
    "notion-api-wrapper",
    "naw",
    "ntn"
  ],
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint",
    "format:write": "prettier --write ./src/**/*.ts",
    "format:check": "prettier --check ./src/**/*.ts",
    "bump:patch": "npm version patch --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:major": "npm version major --no-git-tag-version"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@notionhq/client": "^4.0.1",
    "@types/node": "^24.2.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=20"
  }
}
